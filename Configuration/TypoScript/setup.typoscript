plugin.tx_pxasocialfeed {
    view {
        templateRootPaths {
            10 = {$plugin.tx_pxasocialfeed.view.templateRootPath}
        }

        partialRootPaths {
            10 = {$plugin.tx_pxasocialfeed.view.partialRootPath}
        }

        layoutRootPaths {
            10 = {$plugin.tx_pxasocialfeed.view.layoutRootPath}
        }
    }

    settings {
        dateFormat = %d-%m-%y
    }
}

page.includeCSS {
    pxa_social_feed_1 = EXT:pxa_social_feed/Resources/Public/Css/socialFeed.css
}

page.includeJSFooterlibs {
    pxa_social_feed_1 = https://code.jquery.com/jquery-2.2.4.min.js
    pxa_social_feed_2 = EXT:pxa_social_feed/Resources/Public/JavaScript/jquery.dotdotdot.min.js
    pxa_social_feed_3 = EXT:pxa_social_feed/Resources/Public/JavaScript/owl.carousel.min.js
    pxa_social_feed_4 = EXT:pxa_social_feed/Resources/Public/JavaScript/masonry.pkgd.min.js
    pxa_social_feed_5 = EXT:pxa_social_feed/Resources/Public/JavaScript/imagesloaded.pkgd.min.js
}

page.includeJSFooter {
    pxa_social_feed_6 = EXT:pxa_social_feed/Resources/Public/JavaScript/socialFeed.js
}

pxaAjaxFeedLoad = PAGE
pxaAjaxFeedLoad {
    typeNum = 7378121
    config {
        disableAllHeaderCode = 1
    }

    5 = USER_INT
    5 {
        userFunc = TYPO3\CMS\Extbase\Core\Bootstrap->run
        vendorName = Pixelant
        extensionName = PxaSocialFeed
        pluginName = Showfeed

        switchableControllerActions {
            \Pixelant\PxaSocialFeed\Controller\FeedsController {
                1 = loadFeedAjax
            }
        }

        settings =< plugin.tx_pxasocialfeed.settings
    }
}
